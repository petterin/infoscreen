{
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "node",
    "prettier"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "node/exports-style": ["error", "module.exports"],
    "prettier/prettier": "warn"
  }
}
