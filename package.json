{
  "name": "infoscreen",
  "version": "0.1.0",
  "description": "Infoscreen web app",
  "author": "Petteri Noponen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/petterin/infoscreen.git"
  },
  "main": "server/index.js",
  "engines": {
    "node": ">=18.14.0"
  },
  "browserslist": [
    "> 0.1% and last 5 years",
    "Firefox ESR",
    "not op_mini all",
    "not dead"
  ],
  "scripts": {
    "build": "webpack --config build_config/webpack.prod.js",
    "start": "node ./bin/www",
    "watch": "npm run watch:client & npm run watch:server",
    "watch:client": "webpack --watch --config build_config/webpack.dev.js",
    "watch:server": "nodemon --watch server ./bin/www",
    "lint": "eslint ./client ./server",
    "stats": "webpack --profile --json --config build_config/webpack.prod.js > webpack-stats.json",
    "stats:view": "webpack-bundle-analyzer webpack-stats.json public/assets"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "date-fns": "^2.30.0",
    "debug": "^4.3.4",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "graphql-request": "^6.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mqtt": "^5.3.3",
    "node-cache": "^5.1.2",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intl": "^6.4.2",
    "regenerator-runtime": "^0.14.1",
    "weather-icons": "^1.3.2",
    "xml2js": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.4",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "graphql": "^16.6.0",
    "html-webpack-plugin": "^5.5.1",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.31",
    "postcss-custom-properties": "^13.1.5",
    "postcss-loader": "^7.3.1",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "prettier": "~2.8.8",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.84.1",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-cli": "^5.1.1",
    "webpack-merge": "^5.9.0"
  }
