{
  "name": "infoscreen",
  "version": "0.1.0",
  "description": "Infoscreen web app",
  "author": "Petteri Noponen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/petterin/infoscreen.git"
  },
  "main": "server/index.js",
  "engines": {
    "node": ">=12.13.0"
  },
  "browserslist": [
    "> 0.1% and last 5 years",
    "Firefox ESR",
    "not op_mini all",
    "not dead"
  ],
  "scripts": {
    "build": "webpack --config build_config/webpack.prod.js",
    "start": "node ./bin/www",
    "watch": "npm run watch:client & npm run watch:server",
    "watch:client": "webpack --watch --config build_config/webpack.dev.js",
    "watch:server": "nodemon --watch server ./bin/www",
    "lint": "npm run eslint:client && npm run eslint:server",
    "eslint:client": "eslint ./client",
    "eslint:server": "eslint ./server",
    "stats": "webpack --profile --json --config build_config/webpack.prod.js > webpack-stats.json",
    "stats:view": "webpack-bundle-analyzer webpack-stats.json public/assets"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "date-fns": "^2.22.1",
    "debug": "^4.3.1",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "graphql-request": "^3.4.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mqtt": "^4.2.6",
    "node-cache": "^5.1.2",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.20.2",
    "regenerator-runtime": "^0.13.7",
    "weather-icons": "^1.3.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "graphql": "^15.5.0",
    "html-webpack-plugin": "^5.3.1",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^1.6.0",
    "nodemon": "^2.0.7",
    "postcss": "^8.3.2",
    "postcss-custom-properties": "^11.0.0",
    "postcss-loader": "^6.1.0",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.3.1",
    "raw-loader": "^4.0.2",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.3",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  }
}
