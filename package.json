{
  "name": "infoscreen",
  "version": "0.1.0",
  "description": "Infoscreen web app",
  "author": "Petteri Noponen",
  "license": "MIT",
  "main": "server/index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "Chrome >= 56",
    "Firefox ESR",
    "not ie <11"
  ],
  "scripts": {
    "build": "webpack --config build_config/webpack.prod.js",
    "start": "node ./bin/www",
    "watch": "npm run watch:client & npm run watch:server",
    "watch:client": "webpack --watch --debug --config build_config/webpack.dev.js",
    "watch:server": "nodemon --watch server ./bin/www",
    "lint": "npm run eslint:client && npm run eslint:server",
    "eslint:client": "eslint ./client",
    "eslint:server": "eslint ./server",
    "stats": "webpack --profile --json --config build_config/webpack.prod.js > webpack-stats.json",
    "stats:view": "webpack-bundle-analyzer webpack-stats.json public/assets"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cached": "^5.0.1",
    "date-fns": "^1.30.1",
    "debug": "^4.1.1",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "graphql-request": "^1.8.2",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "mqtt": "^3.0.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^2.9.0",
    "weather-icons": "^1.3.2",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-custom-properties": "^9.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.18.2",
    "raw-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-merge": "^4.2.1"
  }
}
