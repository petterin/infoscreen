{
  "name": "infoscreen",
  "version": "0.1.0",
  "description": "Infoscreen web app",
  "author": "Petteri Noponen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/petterin/infoscreen.git"
  },
  "main": "server/index.js",
  "engines": {
    "node": ">=10.13.0"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions",
    "Chrome >= 56",
    "Firefox ESR",
    "not ie <11"
  ],
  "scripts": {
    "build": "webpack --config build_config/webpack.prod.js",
    "start": "node ./bin/www",
    "watch": "npm run watch:client & npm run watch:server",
    "watch:client": "webpack --watch --debug --config build_config/webpack.dev.js",
    "watch:server": "nodemon --watch server ./bin/www",
    "lint": "npm run eslint:client && npm run eslint:server",
    "eslint:client": "eslint ./client",
    "eslint:server": "eslint ./server",
    "stats": "webpack --profile --json --config build_config/webpack.prod.js > webpack-stats.json",
    "stats:view": "webpack-bundle-analyzer webpack-stats.json public/assets"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "cached": "^5.0.2",
    "date-fns": "^2.16.1",
    "debug": "^4.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-request": "^3.1.0",
    "lodash": "^4.17.20",
    "morgan": "^1.10.0",
    "mqtt": "^4.2.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^5.8.2",
    "regenerator-runtime": "^0.13.7",
    "weather-icons": "^1.3.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "autoprefixer": "^10.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.11.2",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.0.5",
    "postcss-custom-properties": "^10.0.0",
    "postcss-loader": "^4.0.2",
    "postcss-nested": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.2",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^4.2.2"
  }
}
