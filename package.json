{
  "name": "infoscreen",
  "version": "0.1.0",
  "description": "Infoscreen web app",
  "author": "Petteri Noponen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/petterin/infoscreen.git"
  },
  "main": "server/index.js",
  "engines": {
    "node": ">=12.22.0"
  },
  "browserslist": [
    "> 0.1% and last 5 years",
    "Firefox ESR",
    "not op_mini all",
    "not dead"
  ],
  "scripts": {
    "build": "webpack --config build_config/webpack.prod.js",
    "start": "node ./bin/www",
    "watch": "npm run watch:client & npm run watch:server",
    "watch:client": "webpack --watch --config build_config/webpack.dev.js",
    "watch:server": "nodemon --watch server ./bin/www",
    "lint": "eslint ./client ./server",
    "stats": "webpack --profile --json --config build_config/webpack.prod.js > webpack-stats.json",
    "stats:view": "webpack-bundle-analyzer webpack-stats.json public/assets"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "axios": "^0.25.0",
    "body-parser": "^1.19.1",
    "date-fns": "^2.28.0",
    "debug": "^4.3.3",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "graphql-request": "^3.7.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mqtt": "^4.3.4",
    "node-cache": "^5.1.2",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.24.3",
    "regenerator-runtime": "^0.13.9",
    "weather-icons": "^1.3.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-lodash": "^3.3.4",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "graphql": "^16.2.0",
    "html-webpack-plugin": "^5.5.0",
    "lodash-webpack-plugin": "^0.11.6",
    "mini-css-extract-plugin": "^2.5.2",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.5",
    "postcss-custom-properties": "^12.1.3",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.2.3",
    "prettier": "2.5.1",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.67.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
