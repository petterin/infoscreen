{
  "parser": "@babel/eslint-parser",
  "extends": [
    "airbnb",
    "prettier",
    "plugin:prettier/recommended",
    "plugin:react-hooks/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": "warn",
    "react/jsx-wrap-multilines": [
      "warn",
      {
        "declaration": false,
        "assignment": false,
        "arrow": false,
        "return": false
      }
    ]
  },
  "overrides": [
    {
      "files": [
        "*.js",
        "*.jsx"
      ]
    }
  ]
}
